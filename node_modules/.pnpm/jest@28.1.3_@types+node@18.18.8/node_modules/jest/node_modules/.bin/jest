#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/jest-cli@28.1.3_@types+node@18.18.8/node_modules/jest-cli/bin/node_modules:/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/jest-cli@28.1.3_@types+node@18.18.8/node_modules/jest-cli/node_modules:/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/jest-cli@28.1.3_@types+node@18.18.8/node_modules:/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/jest-cli@28.1.3_@types+node@18.18.8/node_modules/jest-cli/bin/node_modules:/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/jest-cli@28.1.3_@types+node@18.18.8/node_modules/jest-cli/node_modules:/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/jest-cli@28.1.3_@types+node@18.18.8/node_modules:/mnt/c/xampp/htdocs/alumni-react/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../jest-cli@28.1.3_@types+node@18.18.8/node_modules/jest-cli/bin/jest.js" "$@"
else
  exec node  "$basedir/../../../../../jest-cli@28.1.3_@types+node@18.18.8/node_modules/jest-cli/bin/jest.js" "$@"
fi
