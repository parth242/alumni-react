{
	"compilerOptions": {
		"types": [
			"vite/client"
		],
		"baseUrl": "./src",
		"target": "ESNext",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx",
		/* "paths": {
			"@sounds/*": [
				"src/assets/game-plinko/sounds/*"
			],
			"@images/*": [
				"src/assets/game-plinko/img/*"
			]
		} */
	},
	"include": [
		"src",
		"public/assets/config.js"
	],
	"exclude": [
		"node_modules"
	]
}